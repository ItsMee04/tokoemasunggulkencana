	<queryString language="SQL">
		<![CDATA[SELECT 
	pm.kodepembelian,
	pm.tanggal,
	pm.total_harga AS totalpembelian,
	pm.terbilang,
	
	COALESCE(pl.nama, sp.nama, pm.nonsuplierdanpembeli) AS nama_pihak,
	COALESCE(pl.alamat, sp.alamat, '-') AS alamat_pihak,
	COALESCE(pl.kontak, sp.kontak, '-') AS kontak_pihak,
	
	pg.nama AS namapegawai,

	pmr.kodeproduk, 
	pmr.nama AS namaproduk,
	pmr.harga_beli,
	pmr.harga_jual,
	pmr.berat,
	pmr.karat,
	pmr.lingkar,
	pmr.panjang,
	pmr.keterangan,
	k.kondisi,
	pmr.subtotalharga,
	
	(SELECT SUM(subt.subtotalharga)
		FROM `dbtokoemasunggulkencana`.pembelian_produk subt 
		WHERE subt.kodepembelianproduk = pmr.kodepembelianproduk
	) AS total_keranjang
	
	FROM `dbtokoemasunggulkencana`.pembelian pm
	JOIN `dbtokoemasunggulkencana`.pembelian_produk pmr on pm.kodepembelianproduk = pmr.kodepembelianproduk
	
	LEFT JOIN `dbtokoemasunggulkencana`.pelanggan pl ON pm.pelanggan_id = pl.id
	LEFT JOIN `dbtokoemasunggulkencana`.suplier sp ON pm.suplier_id = sp.id
	
	LEFT JOIN `dbtokoemasunggulkencana`.users us ON pm.oleh = us.id
	LEFT JOIN `dbtokoemasunggulkencana`.pegawai pg ON us.pegawai_id = pg.id
	
	LEFT JOIN`dbtokoemasunggulkencana`. kondisi k ON pmr.kondisi_id = k.id
	
	WHERE pm.kodepembelian =  $P{kodepembelian}]]>
	</queryString>
