	<queryString language="SQL">
		<![CDATA[SELECT tr.kodetransaksi, pl.nama AS namapelanggan, pl.alamat, pl.kontak, pg.nama AS namauser, pr.kodeproduk, pr.nama AS namaproduk, pr.berat, pr.karat, pr.harga_jual, pr.image_produk,tr.total,tr.terbilang, pg.nama as namapegawai, kr.total AS keranjangtotal FROM `dbtokoemasunggulkencana`.produk pr
JOIN `dbtokoemasunggulkencana`.keranjang kr ON pr.id = kr.produk_id
JOIN `dbtokoemasunggulkencana`.transaksi tr ON kr.kodekeranjang = tr.kodekeranjang_id
JOIN `dbtokoemasunggulkencana`.pelanggan pl ON tr.pelanggan_id = pl.id
JOIN `dbtokoemasunggulkencana`.diskon ds ON tr.diskon_id = ds.id
JOIN `dbtokoemasunggulkencana`.users us ON tr.oleh = us.id
JOIN `dbtokoemasunggulkencana`.pegawai pg ON us.pegawai_id = pg.id
WHERE tr.kodetransaksi = $P{kodetransaksi}   AND pr.kodeproduk =  $P{kodeproduk}]]>
	</queryString>
